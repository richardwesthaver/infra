;;; ~/.sbclrc --- sbcl user init file -*- mode: common-lisp; -*-
(in-package :cl-user)
#-quicklisp
(let ((quicklisp-init "/usr/local/share/quicklisp/setup.lisp"))
  (when (probe-file quicklisp-init)
    (load quicklisp-init)
    (pushnew (truename "/usr/local/share/lisp/") ql:*local-project-directories*)
    (pushnew (truename "/usr/share/lisp/") ql:*local-project-directories* )))
(require :sb-aclrepl)
